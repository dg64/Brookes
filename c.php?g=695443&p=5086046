    jQuery(function() {
        var hash = window.location.hash;

        // Don't try to acccess garbage parameters
        if (hash.length === 0 || jQuery('.s-lib-jqtabs a[href="' + hash + '"]').get(0) === undefined) {
            return;
        }

        // Show the tab
        jQuery('.s-lib-jqtabs a[href="' + hash + '"]').tab('show');
